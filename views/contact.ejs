<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>
        <%= title %>
    </title>
    <link rel="stylesheet" href="/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="/@fortawesome/fontawesome-free/css/all.min.css" />
    <link rel=' stylesheet ' href='/Content/service.css' />

</head>


<body> 
<%- include partials/main_nav.ejs %>


<!--main part-->
<main>
    <div class="col-md-12 text-center">
        <h3 class="animate-charcter"> Welcome to the <%= title %> page</h3>
    </div>
    <div class="wrapper">
        <form class="form" active="index.ejs" method="GET">
            <label id="fname">
                <input type="text" name="fname" placeholder="First name" required/>
            </label>

            <label id="lname">
                <input type="text" name="lname" placeholder="Last name" required/>
            </label>

            <label id="phone">
                <input type="tel" name="phone" placeholder="Phone number" required/>
            </label>

            <label id="email">
                <input type="text" name="email" placeholder="Email Adress" required/>
            </label>

            <label id="message">
                <input type="text" name="message" row="5" placeholder="Message" required/>
            </label>

            

            <input type="submit" value="submit" name="submit"/>
        </form>
    </div>
    

    


</main>



<%- include partials/bottom_nav.ejs %>

<%- include partials/footer.ejs %>


